import java.util.Scanner;

public class AsciiShop{
	public static void main(String[] args){
			
		Scanner sc = new Scanner(System.in);
		
		int height = 0;
		int width = 0;
		String line = "";
		boolean error = false;
		
		if(!sc.hasNext){
			error = true;
			}
		
		while(sc.hasNext){
			
			sc.next() = line;
			width = line.length();
			height++;
			}
		}
	}